{
  "name": "schiba",
  "version": "0.2.2",
  "description": "Database schema extraction tool with connection management",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "schiba": "bin/cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "pnpm run clean",
    "build": "vite build && tsc --emitDeclarationOnly",
    "start": "node dist/index.js",
    "dev": "vite",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run build",
    "release:patch": "pnpm version patch && git push --follow-tags",
    "release:minor": "pnpm version minor && git push --follow-tags",
    "release:major": "pnpm version major && git push --follow-tags"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "commander": "^11.0.0",
    "dotenv": "^16.0.0",
    "gpt-tokenizer": "^2.8.1",
    "mongodb": "^6.0.0",
    "ora": "^7.0.1",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "5.3.3",
    "vite": "^4.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
